[access "refs/*"]
	owner = group Administrators
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group Crespo Maintainers
	label-Verified = -1..+1 group Crespo Maintainers
	submit = group Crespo Maintainers
	forgeCommitter = group Crespo Maintainers
	push = +force group Crespo Maintainers
	create = group Crespo Maintainers
[project]
	state = active
